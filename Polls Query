--Create 20 Year table
Create Table historical(
    year integer,
    statename varchar,
    state_abbv varchar,
    countyname varchar,
    countyfips varchar,
    office varchar,
    party varchar,
    candidate varchar,
    candidatevotes integer,
    totalvotes integer,
    version integer,
    mode varchar
);

--select * from historical;

Create Table Pres2016(
    year integer,
    stage varchar,
    special boolean,
    statename varchar,
    state_abbv varchar,
    statefips varchar,
    stateicpsr varchar,
    countyname varchar,
    countyfips varchar,
    countyansi varchar,
    county_lat varchar,
    county_long varchar,
    jurisdiction varchar,
    precinct varchar,
    candidatename varchar,
    candidatenormal varchar,
    office varchar,
    district varchar,
    writein boolean,
    party varchar,
    mode varchar,
    votes integer,
    opensecrets varchar,
    wikidata varchar,
    can_party varchar,
    can_last varchar,
    can_first varchar,
    can_middle varchar,
    can_full varchar,
    can_suff varchar,
    can_nick varchar,
    can_fec varchar,
    can_fec_name varchar,
    can_google varchar,
    can_govtrack varchar,
    can_icpsr varchar,
    can_maplight varchar
);

--select * from Pres2016
--LIMIT 100;

Create Table Pres2020(
    precinct varchar,
    office varchar,
    party_detail varchar,
    party_simple varchar,
    mode varchar,
    votes integer,
    countyname varchar,
    countyfips varchar,
    jurisname varchar,
    juris_fips varchar,
    candidatename varchar,
    district varchar,
    magnitude integer,
    dataverse varchar,
    year integer,
    stage varchar,
    statename varchar,
    special boolean,
    writein boolean,
    state_abbv varchar,
    statefips integer,
    state_cen integer,
    state_ic integer,
    electdate date,
    readme boolean     
);

--select * from Pres2020
--LIMIT 100;
